{
    "compilerOptions": {
        "target": "es6",
        "experimentalDecorators": true,
        "baseUrl": ".",
        "paths": {
            "vue$/*": [
                "vue/dist/vue.esm.js"
            ],
            "@/*": [
                "./src/*"
            ],
            "@assets": [
                "./src/assets/*"
            ],
            "@components/*": [
                "./src/components/*"
            ],
            "@config/*": [
                "./src/config/*"
            ],
            "@service/*": [
                "./src/service/*"
            ],
            "@mixins/*": [
                "./src/mixins/*"
            ],
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "include": [
        "./src/**/*",
    ],
}
